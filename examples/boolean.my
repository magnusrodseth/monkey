let evaluate = fn (value) {
    if (value) {
      print(str(value) + " is truthy");
    } else {
      print(str(value) + " is falsy");
    }
}

evaluate(true);
evaluate(false);
evaluate(5);
evaluate(-5);
evaluate(0);
evaluate("");
evaluate("hello");

